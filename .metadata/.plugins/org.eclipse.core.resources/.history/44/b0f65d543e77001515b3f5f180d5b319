
public class WhileLoops {

	public static void main(String[] args) 
	{
		int i = 10;
		
		System.out.println("While loop count down ");
		
		while(i > 0)
		{
			System.out.println(i);
			i = i - 1;
		}
		
		System.out.println("For loop count down ");
		
		for(int l = 10; l > 0; l--)
		{
			System.out.println(l);
		
		}
	}
}